    <script src="{{ asset('assets/appfront/js/jquery.min.js') }}"></script>
    <!-- Popper -->
    <script src="{{ asset('assets/appfront/js/popper.min.js') }}"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('assets/appfront/js/bootstrap.min.js') }}"></script>
    <!-- All Plugins -->
    <script src="{{ asset('assets/appfront/js/rehomes.bundle.js') }}"></script>
    <!-- Active -->
    <script src="{{ asset('assets/appfront/js/default-assets/active.js') }}"></script>

