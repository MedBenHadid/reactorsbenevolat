{% extends '::baseback.html.twig' %}
{% block title %}Modification:{{ association.nomAssociation }} {% endblock %}
{% block info %}Modification de l'association <b>{{ association.nomAssociation }}</b>:{% endblock %}
{% block bread %}<a href="{{ path('association_index') }}">Association</a> /{{ association.nomAssociation }} {% endblock %}
{% block body %}
    <h1>Association edit</h1>

    {{ form_start(edit_form) }}
        {{ form_widget(edit_form) }}
        <input type="submit" value="Edit" />
    {{ form_end(edit_form) }}

    <ul>
        <li>
            <a href="{{ path('association_index') }}">Back to the list</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
